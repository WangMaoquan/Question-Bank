{
  "name": "@question-bank/types",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --tsconfig tsconfig.json",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch --tsconfig tsconfig.json",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.7.3"
  }
}